Private Sub txtMenuNo_KeyDown(KeyCode As Integer, Shift As Integer)

    '' Enter キーが押された場合
    If KeyCode = vbKeyReturn Then
        Call btnMenu_Click(Val(txtMenuNo.Text) - 1)
    End If

End Sub
