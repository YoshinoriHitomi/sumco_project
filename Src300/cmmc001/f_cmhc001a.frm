'概要      :メニューボタンクリック処理
'ﾊﾟﾗﾒｰﾀ    :変数名        ,IO ,型        ,説明
'          :Index         ,I   ,Integer   ,メニューボタンインデックス番号
'説明      :
Private Sub btnMenu_Click(Index As Integer)

    Select Case Index
    Case 0
        ShowFormProc Me, f_cmmc001a
    End Select

    '' メニューNo.にフォーカスセット
    txtMenuNo.Text = ""

End Sub

'概要      :ファンクションボタンクリック処理
'ﾊﾟﾗﾒｰﾀ　　:変数名　　　　,IO ,型       ,説明
'　　      :Index        ,I  ,Integer　,コントロール配列の添字
'説明      :ファンクションボタンがクリックされたら、各処理に分岐する
Private Sub cmdF_Click(Index As Integer)

    Select Case Index
    Case 1          '' Ｆ１キー（メインメニュー）
        Unload Me
    End Select


End Sub


'概要      :キーボード押下処理
'ﾊﾟﾗﾒｰﾀ　　:変数名　　　　,IO ,型       ,説明
'　　      :KeyCode      ,I  ,Integer　,キーコード
'　　      :Shift        ,I  ,Integer　,Shiftキーの状態
'説明      :キーが押されたら、各処理に分岐する
Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
    
    Select Case KeyCode
    Case vbKeyF1    '' Ｆ１キー
        Call cmdF_Click(1)
    Case vbKeyF2    '' Ｆ２キー
        Call cmdF_Click(2)
    Case vbKeyF3    '' Ｆ３キー
        Call cmdF_Click(3)
    Case vbKeyF4    '' Ｆ４キー
        Call cmdF_Click(4)
    Case vbKeyF5    '' Ｆ５キー
        Call cmdF_Click(5)
    Case vbKeyF6    '' Ｆ６キー
        Call cmdF_Click(6)
    Case vbKeyF7    '' Ｆ７キー
        Call cmdF_Click(7)
    Case vbKeyF8    '' Ｆ８キー
        Call cmdF_Click(8)
    Case vbKeyF9    '' Ｆ９キー
        Call cmdF_Click(9)
    Case vbKeyF10   '' Ｆ１０キー
        Call cmdF_Click(10)
    Case vbKeyF11   '' Ｆ１１キー
        Call cmdF_Click(11)
    Case vbKeyF12   '' Ｆ１２キー
        Call cmdF_Click(12)
    End Select

    '' メニューNo.にフォーカスセット
    txtMenuNo.SetFocus

End Sub

'概要      :フォームのロード
'説明      :
Private Sub Form_Load()
    InitExe
    
    CenterForm Me
    Me.Show
    txtMenuNo.Text = ""
    txtMenuNo.SetFocus
End Sub

Private Sub Form_Unload(Cancel As Integer)
    '' プログラム終了処理
    ExitExe MAINMENU_PGNAME
End Sub

